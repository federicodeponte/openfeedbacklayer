{
  "name": "openfeedbacklayer",
  "version": "0.1.0",
  "description": "Open-source feedback widget with AI classification. Drop-in component for Next.js apps.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./widget": {
      "import": "./dist/widget.mjs",
      "require": "./dist/widget.js",
      "types": "./dist/widget.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "supabase"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "feedback",
    "widget",
    "ai",
    "classification",
    "nextjs",
    "react",
    "supabase",
    "gemini"
  ],
  "author": "Federico De Ponte",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/federicodeponte/openfeedbacklayer.git"
  },
  "homepage": "https://github.com/federicodeponte/openfeedbacklayer#readme",
  "bugs": {
    "url": "https://github.com/federicodeponte/openfeedbacklayer/issues"
  }
}
